{
    "name":			"ASP-example",
	"description":	"This is example profile. DO NOT use it in Production without prior review and testing",
	"rules":		[
		{
			"name":				"Block All High-to-Critical",
			"action":			"drop",
			"severity":			["critical", "high"],
			"threat-name":		"any",
			"category":			"any",
			"packet-capture":	"extended-capture"
		},
		{
			"name":				"Block Med-to-Critical Hacktools",
			"action":			"reset-both",
			"severity":			["critical", "high", "medium"],
			"threat-name":		"any",
			"category":			"hacktool",
			"packet-capture":	"extended-capture"
		},
		{
			"name":				"Default all other severity",
			"action":			"default",
			"severity":			["medium", "low", "informational"],
			"threat-name":		"any",
			"category":			"any",
			"packet-capture":	"disable"			
		}	
	],
	"botnet-domains":	{
		"dns-security-categories":	[
			{
				"name":				"pan-dns-sec-adtracking",
